#include <iostream>
#include <string>
using namespace std;

void countDuplicates(int* arr, int size) {
    bool* processed = new bool[size]();

    for (int i = 0; i < size; i++) {
        if (processed[i]) continue;

        int count = 1;
        for (int j = i + 1; j < size; j++) {
            if (arr[i] == arr[j]) {
                count++;
                processed[j] = true;
            }
        }

        if (count > 1) {
            cout << arr[i] << ": " << count << endl;
        }
    }

    delete[] processed;
}

int main() {
    int size;
    cout << "Enter array size: ";
    cin >> size;
    int* arr = new int[size];
    cout << "Enter " << size << " numbers: " << endl;
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
    }

    cout << "Duplicate elements: " << endl;
    countDuplicates(arr, size);

    delete[] arr;
    return 0;
}
