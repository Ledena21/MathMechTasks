#include <iostream>
#include <string>
using namespace std;

template <typename T>
void countDuplicates(T* arr, int size) {
    bool* processed = new bool[size]();

    for (int i = 0; i < size; i++) {
        if (processed[i]) continue;

        int count = 1;
        for (int j = i + 1; j < size; j++) {
            if (arr[i] == arr[j]) {
                count++;
                processed[j] = true;
            }
        }

        if (count > 1) {
            cout << arr[i] << ": " << count << " times" << endl;
        }
    }

    delete[] processed;
}

int main() {
    int intArr[] = { 1, 2, 3, 2, 4, 3, 5 };
    cout << "Duplicate integers:" << endl;
    countDuplicates(intArr, 7);

    string strArr[] = { "apple", "banana", "apple", "orange", "orange"};
    cout << "\nDuplicate strings:" << endl;
    countDuplicates(strArr, 5);

    return 0;
}
